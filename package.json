{
  "name": "havenride",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "prisma:generate": "prisma generate",
    "prisma:migrate": "prisma migrate dev",
    "prisma:seed": "tsx prisma/seed.ts",
    "mobile:dev": "CAPACITOR_SERVER_URL=http://localhost:3000 npx cap sync",
    "mobile:build": "npm run build && npm run mobile:sync",
    "mobile:build:dev": "npm run build && npm run mobile:dev",
    "mobile:ios": "npx cap open ios",
    "mobile:ios:pod": "cd ios/App && pod install",
    "mobile:ios:run": "bash scripts/ios-run.sh",
    "mobile:ios:run:simulator": "bash scripts/ios-run.sh",
    "mobile:ios:simulator": "xcrun simctl list devices available | grep -E '(iPhone|iPad)' | head -15",
    "mobile:ios:run:dev": "npm run mobile:dev && npm run mobile:ios:run",
    "mobile:android": "npx cap open android",
    "mobile:sync": "npx cap sync",
    "mobile:copy": "npx cap copy",
    "mobile:start": "npm run dev & sleep 3 && npm run mobile:dev"
  },
  "dependencies": {
    "@capacitor/android": "^7.4.4",
    "@capacitor/app": "^7.1.0",
    "@capacitor/cli": "^7.4.4",
    "@capacitor/core": "^7.4.4",
    "@capacitor/haptics": "^7.0.2",
    "@capacitor/ios": "^7.4.4",
    "@capacitor/keyboard": "^7.0.3",
    "@capacitor/splash-screen": "^7.0.3",
    "@capacitor/status-bar": "^7.0.3",
    "@clerk/nextjs": "^6.0.0",
    "@mapbox/mapbox-gl-geocoder": "^5.1.2",
    "@prisma/client": "^6.18.0",
    "@stripe/react-stripe-js": "^5.3.0",
    "@stripe/stripe-js": "^8.3.0",
    "ably": "^2.14.0",
    "mapbox-gl": "^3.16.0",
    "next": "^15.5.6",
    "react": "^18.3.0",
    "react-dom": "^18.3.0",
    "stripe": "^17.0.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.38.0",
    "@next/eslint-plugin-next": "^16.0.1",
    "@tailwindcss/postcss": "^4.1.16",
    "@types/node": "^22.0.0",
    "@types/react": "^18.3.0",
    "@types/react-dom": "^18.3.0",
    "autoprefixer": "^10.4.21",
    "eslint": "^9.38.0",
    "eslint-config-next": "16.0.0",
    "postcss": "^8.5.6",
    "prisma": "^6.18.0",
    "tailwindcss": "^4.1.16",
    "tsx": "^4.20.6",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.46.2"
  },
  "prisma": {
    "seed": "tsx prisma/seed.ts"
  }
}
